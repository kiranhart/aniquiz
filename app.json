{
  "expo": {
    "name": "AniQuiz",
    "slug": "aniquiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon-32.png",
    "scheme": "aniquiz",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/apple-touch-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ca.tweetzy.aniquiz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/icon-32.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
